{
  "name": "md-editor-rt",
  "version": "2.10.1",
  "keywords": [
    "react",
    "nextjs",
    "javascript",
    "typescript",
    "jsx",
    "tsx",
    "ssr",
    "markdown",
    "editor",
    "theme",
    "html"
  ],
  "description": "Markdown editor for react, developed in jsx and typescript, dark theme、beautify content by prettier、render articles directly、paste or clip the picture and upload it...",
  "author": {
    "name": "zbf",
    "url": "https://imzbf.cc"
  },
  "homepage": "https://imzbf.github.io/md-editor-rt/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imzbf/md-editor-rt.git"
  },
  "bugs": {
    "url": "https://github.com/imzbf/md-editor-rt/issues"
  },
  "files": [
    "lib"
  ],
  "type": "module",
  "types": "./lib/MdEditor/index.d.ts",
  "main": "./lib/md-editor-rt.cjs",
  "module": "./lib/md-editor-rt.mjs",
  "exports": {
    ".": {
      "import": "./lib/md-editor-rt.mjs",
      "require": "./lib/md-editor-rt.cjs"
    },
    "./lib/style.css": "./lib/style.css"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/preset-react": "^7.18.6",
    "@types/katex": "^0.16.0",
    "@types/marked": "^4.0.8",
    "@types/multiparty": "^0.0.33",
    "@types/node": "^18.15.3",
    "@types/prettier": "^2.7.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vavt/markdown-theme": "^2.2.2",
    "@vitejs/plugin-react": "^3.1.0",
    "axios": "^1.3.4",
    "copy-to-clipboard": "^3.3.3",
    "cropperjs": "^1.5.13",
    "eslint": "^8.36.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "highlight.js": "^11.7.0",
    "katex": "^0.16.4",
    "less": "^4.1.3",
    "lru-cache": "^8.0.0",
    "marked": "^4.2.12",
