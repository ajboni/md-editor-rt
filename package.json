    "multiparty": "^4.2.3",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "screenfull": "^6.0.2",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-plugin-dts": "^2.1.0"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none",
